create database toko_buku;
use toko_buku;
create table supplier(
     ID_supplier int(12) primary key,
     nama varchar(255),
     telp int(15),
     email varchar(255));
create table katalog(
     ID_buku int(10) primary key,
     ISBN int(10),
     judul varchar(255),
     pengarang varchar(255),
     penerbit varchar(255),
     tahun_terbit date,
     stock int(255),
     harga_jual varchar(255),
     harga_beli varchar(255),
     ID_supplier int(10),
     foreign key(ID_supplier) references supplier(ID_supplier));
create table pelanggan(
     ID_pelanggan int(10) primary key,
     nama varchar(255),
     telp int(15),
     alamat varchar(255),
     email varchar(255),
     no_kartu_kredit varchar(10));
 create table akun(
     ID_akun int(10) primary key,
     ID_pelanggan int(10),
     username varchar(50),
     password varchar(50),
     foreign key(ID_pelanggan) references pelanggan(ID_pelanggan));
create table kurir(
     ID_kurir int(10) primary key,
     nama varchar(255),
     telp int(15),
     email varchar(255));
create table pemesanan(
     ID_pemesanan int(10) primary key,
     ID_buku int(10),
     tanggal date,
     jumlah int(255),
     ID_pelanggan int(10),
     alamat_kirim varchar(255),
     foreign key(ID_buku) references katalog(ID_buku),
     foreign key(ID_pelanggan) references pelanggan(ID_pelanggan));
create table pengiriman(
     ID_pengiriman int(10) primary key,
     ID_pemesanan int(10),
     tanggal_kirim date,
     tanggal_terima date,
     status varchar(255),
     no_resi int(12),
     ID_kurir int(10),
     foreign key(ID_pemesanan) references pemesanan(ID_pemesanan),
     foreign key(ID_kurir) references kurir(ID_kurir));
